.i 3		# .i specifies the number of inputs
.o 3		# .o specifies the number of outputs
.ilb *S1 *S0 in	# This line specifies the names of the inputs in order
.ob *S1 *S0 out	# This line specifies the names of the outputs in order

000 000		# The first four digits (before the space) correspond
001 010		# to the inputs, the three after the space correspond
010 000		# to the outputs, both in order specified above.
011 101
100 010
101 111
110 101
111 111
